      $sc_$cpu_DS_PARAM_ENTRY_TYPE FilterParams[0 .. DS_FILTERS_PER_PACKET-1]
   END
END_TYPES

PACKET Pxxxx APID=nnnn, DESC="$sc $cpu DS Packet Filter Table"
!
#include "cfe_file_header.rdl"
